# .env — ОБЩЕЕ (можно коммитить)
# Храним:
# имена приложения
# флаги
# дефолтные значения
# НЕ секреты
APP_NAME=MyApp
APP_ENV=base
ENABLE_LOGS=true

# В браузер попадает ТОЛЬКО:
# VITE_*
# NEXT_PUBLIC_*
# Любая переменная с этим префиксом видна в браузере
#  НИКОГДА не клади туда секреты

VITE_API_URL=https://api.example.com
NEXT_PUBLIC_SITE_URL=https://example.com
